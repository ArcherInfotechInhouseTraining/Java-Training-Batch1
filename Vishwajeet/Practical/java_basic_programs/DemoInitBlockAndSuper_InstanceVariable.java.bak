class Base
{
	public int a;
	Base()
	{
		a=10;
	}
}
class Derived extends Base
{
	public int a;
	Derived()
	{
		a=100;
	}
	public void out()
	{
		System.out.println("\n In Derived Inh a="+super.a+" and a="+a);
	}
}
class DemoInitBlockAndSuper_InstanceVariable
{
	public static void main(String []args)
	{
		Derived ob=new Derived();
		ob.out();
	}
}